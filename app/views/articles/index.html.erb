<%= form_tag articles_search_path, method: :get, class: "search-form" do %>
  <%= text_field_tag(
      :search,
      params[:search],
      placeholder: "Search",
      class: "form-control"
  ) %>
  <br>
  <%= submit_tag("search") %>
  <br>
  <%= datetime_select(
  :start_time,
  params[:start_time],
  class: "form-control"
  ) %>
  <%= datetime_select(
  :end_time,
  params[:end_time],
  class: "form-control"
  ) %>
<% end %>
